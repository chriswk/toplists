<h2>Show Country</h2>

<ul>

  <li>
    <strong>Iso2:</strong>
    <%= @country.iso2 %>
  </li>

  <li>
    <strong>Iso3:</strong>
    <%= @country.iso3 %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @country.name %>
  </li>

</ul>

<span><%= link "Edit", to: country_path(@conn, :edit, @country) %></span>
<span><%= link "Back", to: country_path(@conn, :index) %></span>
